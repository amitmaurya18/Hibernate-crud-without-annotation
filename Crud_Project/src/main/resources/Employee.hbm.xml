<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.traqiq.Employee" table="emp203">
<!--        <meta attribute="class-description"> This class contains employee details. </meta>-->
        <id name="empno" type="integer" column="empno">
            <generator class="assigned"> </generator>
        </id>
        <property name="ename" type="string" column="ename" length="30" />
        <property name="salary" type="double" column="salary"/>
    </class>
</hibernate-mapping>